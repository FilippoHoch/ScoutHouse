      "get": {
        "tags": [
          "structures"
        ],
        "summary": "List Structures",
        "operationId": "list_structures_api_v1_structures__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/StructureRead"
                  },
                  "type": "array",
                  "title": "Response List Structures Api V1 Structures  Get"
                }
              }
            }
          }
        }
      },
