{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "types": [
      "vitest/globals",
      "vite/client",
      "@testing-library/jest-dom/vitest",
      "google.maps"
    ],
    "paths": {
      "@react-leaflet/core/lib/*": ["./node_modules/@react-leaflet/core/lib/*"]
    }
  },
  "include": ["src"],
  "references": []
}
